generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  email      String   @unique
  hash       String
  hashedRt   String?

  @@map("users")
}

model Puppys {
  id        Int    @id @default(autoincrement())
  puppyName String
  breed     String
}
